@model IEnumerable<DemoApplication.Models.ApplicationType>


<div class="container p-3">
    <div class="row pt-4">
        <div class="col-md-6">
            <h2>Application List</h2>
        </div>
        <div class="col-md-6">
            <a  asp-action="Create" class="btn btn-primary">
                Create New ApplicationType
            </a>
        </div>
    </div>
    <br /><br />

    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped" style="width:100px;">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach(var obj in Model)
                {
                    <tr>
                        <td width="20%">
                            @obj.Name
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    }
            else
            {
                <p>
                    No Application exists
                </p>
            }
</div>

